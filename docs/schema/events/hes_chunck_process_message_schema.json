{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://aws-quickstart.github.io/quickstart-aws-utility-meter-data-analytics-platform/meterdata",
  "title": "meterdata",
  "description": "A meter reading as received from input systems broken down to a single reading per record",
  "type": "object",
  "properties": {
    "bucket": {
      "type": "string",
      "description": "The name of the bucket containing late arriving events",
      "examples": [
        "raw-data-bucket"
      ]
    },
    "prefix": {
      "type": "string",
      "description": "The prefix of the written object",
      "examples": [
        "readings/json/year=2022/month=06/day=01/hour=02/object-containing-json-records"
      ]
    },
    "start-range-bytes": {
      "type": "integer",
      "description": "The start range in bytes. Defaults to 0 (i.e beginning of file). Ranges from 0 inclusive.",
      "minimum": 0,
      "default": 0,
      "examples": [
        0, 3000, 512000
      ]
    },
    "end-range-bytes": {
      "type": "integer",
      "description": "The end range in bytes. Defaults to -1 (i.e. similar to any negative value, means end of file).",
      "default": -1,
      "examples": [
        -1, 400, 30512, 5120001
      ]
    },
    "object-format": {
      "type": "string",
      "description": "The fie format",
      "examples": [
        "CSV", "MRASCO", "TEXT"
      ]
    },
    "arrive_time": {
      "type": "string",
      "format": "date-time",
      "description": "The event publishing time",
      "examples": [
        "yyyy-MM-dd HH:mm:ss.SSS"
      ]
    }
  },
  "required": [
    "bucket",
    "prefix",
    "object-format"
  ]
}